{
  "name": "redux-offline-middleware",
  "version": "1.0.0",
  "description": "Effortlessly store your reducer state offline",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run build:commonjs && npm run build:umd && npm run build:umd:min",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:umd": "cross-env BABEL_ENV=commonjs NODE_ENV=development webpack",
    "build:umd:min": "cross-env BABEL_ENV=commonjs NODE_ENV=production webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/taylorc93/redux-offline-middleware.git"
  },
  "keywords": [
    "redux",
    "offline",
    "indexedDB",
    "localforage",
    "middleware"
  ],
  "author": "Connor Taylor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/taylorc93/redux-offline-middleware/issues"
  },
  "homepage": "https://github.com/taylorc93/redux-offline-middleware#readme",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.6.5",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-register": "^6.11.6",
    "cross-env": "^1.0.7",
    "eslint": "^1.10.2",
    "eslint-config-airbnb": "1.0.2",
    "eslint-plugin-react": "^4.1.0",
    "redux": "^3.4.0",
    "webpack": "^1.11.0"
  },
  "dependencies": {
    "localforage": "^1.4.2"
  }
}
